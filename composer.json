{
    "name": "smoq/simsy-cms-bundle",
    "description": "A simple & easy CMS bundle to drop into a Symfony application",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Smoq\\SimsyCMS\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Le Flem Paul",
            "email": "paul.leflem@icloud.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "symfony/orm-pack": "^2.4",
        "symfony/framework-bundle": "^7.1",
        "symfony/form": "^7.1",
        "symfony/asset-mapper": "^7.1",
        "symfony/asset": "^7.1",
        "symfony/twig-pack": "^1.0",
        "symfony/ux-turbo": "^2.20"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "phpstan/phpstan": "^1.10"
    }
}
